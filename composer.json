{
    "name": "kris/laravel-form-builder",
    "description": "Laravel form builder - symfony like",
    "keywords": ["laravel", "form", "builder","symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kristijan Husak",
            "email": "husakkristijan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laravelcollective/html": "^6",
        "illuminate/database": "^6 || ^7 || ^8 || ^9 || ^10",
        "illuminate/validation": "^6 || ^7 || ^8 || ^9 || ^10"
    },
    "require-dev": {
        "orchestra/testbench": "^6.13 || ^7.0 || ^8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.52-dev"
        },
        "laravel": {
            "providers": [
                "Kris\\LaravelFormBuilder\\FormBuilderServiceProvider"
            ],
            "aliases": {
                "FormBuilder": "Kris\\LaravelFormBuilder\\Facades\\FormBuilder"
            }
        }
    },
    "autoload": {
        "psr-0": {
            "Kris\\LaravelFormBuilder": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
