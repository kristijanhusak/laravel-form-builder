{
    "name": "kris/laravel-form-builder",
    "description": "Laravel form builder - symfony like",
    "keywords": ["laravel", "form", "builder", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kristijan Husak",
            "email": "husakkristijan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "rdx/laravelcollective-html": "^6",
        "illuminate/database": "^10 || ^11 || ^12",
        "illuminate/validation": "^10 || ^11 || ^12"
    },
    "require-dev": {
        "orchestra/testbench": "^8 || ^9 || ^10",
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Kris\\LaravelFormBuilder\\FormBuilderServiceProvider"
            ],
            "aliases": {
                "FormBuilder": "Kris\\LaravelFormBuilder\\Facades\\FormBuilder"
            }
        }
    },
    "autoload": {
        "psr-0": {
            "Kris\\LaravelFormBuilder": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
