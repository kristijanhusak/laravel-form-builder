{
    "name": "mikemnd/laravel-form-builder",
    "description": "Laravel form builder - symfony like",
    "license": "MIT",
    "authors": [
        {
            "name": "Mihail Ilinov",
            "email": "husakkristijan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "mikemnd/html": "5.*",
        "illuminate/database": "5.*@dev",
        "illuminate/validation": "5.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "~3.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.12.x-dev"
        }
    },
    "repositories":[
        {
            "type": "git",
            "url": "git@github.com:MikeMnD/html.git"
        },
    ],
    "autoload": {
        "psr-0": {
            "Kris\\LaravelFormBuilder": "src/"
        },
        "classmap": [
            "tests/FormBuilderTestCase.php"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
